  "version": "0.2.0",
  "configurations": [
     {
      "name": "workmate_app",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "args": ["-d", "chrome", "--web-browser-flag", "disable-web-security"]
    },
    {
      "name": "workmate_app (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": ["-d", "chrome", "--web-browser-flag", "disable-web-security"]
    },
    {
      "name": "workmate_app (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": ["-d", "chrome", "--web-browser-flag", "disable-web-security"]
    }
  ]
}
